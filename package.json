{
  "name": "eslint-changeset",
  "version": "0.1.5",
  "description": "Runs ESLint on changed files",
  "author": "Joshua David <@jdanil>",
  "license": "MIT",
  "homepage": "https://github.com/jdanil/eslint-changeset",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdanil/eslint-changeset.git"
  },
  "bugs": {
    "url": "https://github.com/jdanil/eslint-changeset/issues"
  },
  "main": "./src",
  "bin": "./bin/eslint-changeset.js",
  "types": "./src",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "keywords": [
    "branch",
    "changed",
    "changeset",
    "delta",
    "diff",
    "eslint",
    "git",
    "lint"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "audit-ci": "audit-ci --config .auditcirc.json",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "format": "prettier",
    "format:all": "yarn format .",
    "format:diff": "pretty-quick",
    "libyear": "libyear",
    "lint": "eslint --cache",
    "lint:all": "yarn lint \"**/*\"",
    "lint:diff": "ts-node ./bin/eslint-changeset",
    "prepack": "tsc --build --force",
    "prepare": "husky install",
    "test": "jest --config .jestrc.json --passWithNoTests",
    "test:ci": "yarn test --ci",
    "test:coverage": "yarn test --coverage",
    "test:precommit": "yarn test --bail --findRelatedTests",
    "test:pull-request": "yarn test:ci --changedSince=\"master\""
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "mri": "^1.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/eslint": "^7.2.8",
    "@types/mri": "^1.1.0",
    "@types/node": "^18.11.14",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "audit-ci": "^4.0.0",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "libyear": "^0.6.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "resolutions": {
    "jest-changed-files": "~26.1.0"
  }
}
